{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "new-cap": 0,
        "object-curly-spacing": ["error", "always", {
            "objectsInObjects": false,
            "arraysInObjects": false
        }]
    }
}
